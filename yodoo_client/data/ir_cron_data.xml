<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Creating schedulers -->
    <record id="clear_temporary_auth_data" model="ir.cron">
        <field name="name">Clear temporary auth data</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model" eval="'odoo.infrastructure.client.auth'"/>
        <field name="function" eval="'scheduler_cleanup_expired_entries'"/>
    </record>
</odoo>

